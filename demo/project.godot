;   param=value ; assign values to parameters

config_version=5

[application]

config/name="Godot 4 Visibility Masking Demo"
run/main_scene="res://scenes/etc/title.tscn"
config/features=PackedStringArray("4.2")
config/icon="res://icon.png"

[autoload]

InputMapMgr="*res://utils/input_map/input_map_mgr.gd"
VolumeSettingsMgr="*res://utils/volume_settings_mgr.gd"
TransitionMgr="*res://utils/transition_mgr.tscn"
ScreenshotMgr="*res://utils/screenshot_mgr.tscn"
Globals="*res://addons/godot_helper_pack/core/globals.gd"
SignalMgr="*res://addons/godot_helper_pack/core/signal_mgr.gd"
ServiceMgr="*res://addons/godot_helper_pack/core/service_mgr.gd"
GlobalBlockingColorChangeMonitor="*res://addons/godot_helper_pack/core/2d/shape/global_blocking_color_monitor.tscn"

[debug]

gdscript/warnings/unused_class_variable=true

[display]

window/size/viewport_width=1920
window/size/viewport_height=1080
window/stretch/mode="canvas_items"

[editor_plugins]

enabled=PackedStringArray("res://addons/godot_helper_pack/plugin.cfg")

[global]

monitor=false
transition_mgr_default_speed=0.5
screenshot_mgr_max_images=1000
screenshot_mgr_resize_factor=1.0
screenshot_mgr_toggle_action_name="toggle_screenshots"
screenshot_mgr_frame_delay=0.1

[godot_helper_pack_plugin]

blocking_global_color=Color(1, 1, 1, 1)

